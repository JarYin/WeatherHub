{
  "name": "backend",
  "module": "src/server.ts",
  "main": "dist/server.js",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/server.ts",
    "dev:nodemon": "bunx nodemon --watch src --ext ts,js --exec \"bun run ./src/server.ts\"",
    "build": "bunx tsc && node add-extensions.mjs",
    "start": "node dist/server.js",
    "lint": "eslint src/**/*.ts",
    "test": "bun test"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/json2csv": "^5.0.7",
    "@types/morgan": "^1.9.9",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.16"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-fetch": "^2.6.13",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "helmet": "^8.0.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "node-fetch": "2",
    "openmeteo": "^1.2.1",
    "rate-limiter-flexible": "^8.1.0"
  }
